@model CarLease.Feature.Cars.Models.FilterModel.FilterModel
@using Sitecore.Mvc


<div class="container">
    <h2>@Html.Sitecore().Field("Title")</h2>
    <div class="list-group-item-text">
        <button>@Html.Sitecore().Field("All Cars")</button>
        @if (@Model != null)
        {

            foreach (var filter in Model.Filter)
            {
                <button type="button" id="button" value="Button" onclick="test()">@filter</button>
            }

        }
    </div>
</div>

<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-ajax-unobtrusive/3.2.6/jquery.unobtrusive-ajax.min.js" integrity="sha512-DedNBWPF0hLGUPNbCYfj8qjlEnNE92Fqn7xd3Sscfu7ipy7Zu33unHdugqRD3c4Vj7/yLv+slqZhMls/4Oc7Zg==" crossorigin="anonymous"></script>
<script>
   function test()    
{
   $.ajax({
      url: '/api/Filters/GetFilterCars',
       method: "get",
       cache: false,
       success: function () {
                alert('clicked'); 
            }
   })
}

</script>